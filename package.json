{
  "name": "node-postgres-auth",
  "version": "1.0.0",
  "description": "Register & Login API using Node.js, Express, PostgreSQL, Knex, JWT, Bcrypt",
  "main": "src/server.js",
  "scripts": {
    "start": "pm2 start src/server.js --name ASTRO_API",
    "start:socket": "pm2 start src/socket3.js --name ASTRO_SOCKET_API",
    "dev": "nodemon src/server.js",
    "migrate": "knex migrate:latest --knexfile knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.js"
  },
  "dependencies": {
    "agora-access-token": "^2.0.4",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "firebase-admin": "^13.6.0",
    "fs": "^0.0.1-security",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.12",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "puppeteer": "^24.34.0",
    "razorpay": "^2.9.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
